{
  "name": "Appsys",
  "version": "1.0.0",
  "description": "Appsys",
  "author": {
    "name": "Willem"
  },
  "license": "MIT",
  "scripts": {
    "client": "cd frontend && yarn && yarn start",
    "server": "nodemon server.js",
    "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\"",
    "start": "node server.js",
    "heroku-postbuild": "cd frontend && npm install && npm install --only=dev --no-shrinkwrap && npm run build"
  },
  "dependencies": {
    "react": "^16.8.2",
    "body-parser": "^1.18.3",
    "express": "^4.16.4",
    "pg": "^7.8.1"
  },
  "devDependencies": {
    "concurrently": "^4.1.0"
  },
  "keywords": [
    "node",
    "express",
    "create-react-app"
  ]
}